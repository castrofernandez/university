{{! nothing here }}

<form class="filter">
  <label for="audits">Filter results by audit</label>
  <select id="audit_select" name="audits">
    <option></option>
    {{#each audits}}
        <option value="{{id}}">{{code}}</option>
    {{/each}}
  </select>
</form>

<table class="stats">
  <thead></thead>
  <tbody>
    <tr>
      <td>Number of users</td>
      <td><span id="userCount">{{count}}</span></td>
    </tr>
    <tr>
      <td>Number of unfinished tests</td>
      <td><span id="userUnfinished">{{unfinished}}</span> - (<span id="userUnfinishedPercent">{{unfinishedPercent}}</span>%) <div id="unfinished" class="small-graph"></div></td>
    </tr>
  </tbody>
</table>

<div class="graphs">
  <h3><i class="fa fa-area-chart"></i> Stats</h3>
  <div id="browserGraph" class="graph"></div>
  <div id="ossGraph" class="graph"></div>
</div>

<script src="../../js/stats.js"></script>
